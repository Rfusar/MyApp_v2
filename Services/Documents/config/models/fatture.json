{
    "codice_fiscale": [
        {
            "type": "regex",
            "regex": "\\b[A-Z]{6}[0-9]{2}[A-Z][0-9]{2}[A-Z][0-9]{3}[A-Z]\\b"
        }
    ],
    "partitaIVA": [
        {
            "type": "regex",
            "regex": "(?:P\\.?\\s?IVA|PI|Partita\\sIva)[\\s:]*([0-9]{11})"
        },
        {
            "type": "regex",
            "regex": "\\b[0-9]{11}\\b"
        }
    ],
    "importi": [
        {
            "type": "regex",
            "regex": "€?\\s?([0-9]+(?:[\\.,][0-9]{2})?)\\s?(?:€|euro)?"
        }
    ]
}
